{
    "name": "kilyanov/test_yii",
    "description": "Default Yii 2 Advanced Project for test",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "https://test.loc/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap4": "~2.0.2",
        "yiisoft/yii2-symfonymailer": "~2.0.3",
        "kartik-v/yii2-grid": "dev-master",
        "johnitvn/yii2-ajaxcrud": "^2.1",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/yii2-sortable": "*",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-sortable-input": "*",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-popover-x": "*",
        "kartik-v/yii2-tabs-x": "dev-master",
        "yii2tech/ar-position": "^1.0",
        "trntv/yii2-datetime-widget": "^1.1",
        "2amigos/yii2-date-time-picker-widget": "~1.0",
        "ramsey/uuid": "^4.2.3",
        "thamtech/yii2-uuid": "^1.1",
        "wise5lin/yii2-croppic": "*",
        "phpoffice/phpspreadsheet": "^1.23",
        "hail812/yii2-adminlte3": "~1.1",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev",
        "kartik-v/yii2-widget-depdrop": "dev-master",
        "nesbot/carbon": "^2.14",
        "zircote/swagger-php": "3.0.3",
        "bower-asset/swagger-ui": "^3.20",
        "bizley/jwt": "^3.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "phpunit/phpunit": "~9.5.0",
        "codeception/codeception": "^5.0.0 || ^4.0",
        "codeception/lib-innerbrowser": "^3.0 || ^1.1",
        "codeception/module-asserts": "^3.0 || ^1.1",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem": "^3.0 || ^1.1",
        "codeception/verify": "^2.2",
        "symfony/browser-kit": "^6.0 || >=2.7 <=4.2.4"
    },
    "autoload-dev": {
        "psr-4": {
            "common\\tests\\": ["common/tests/", "common/tests/_support"],
            "backend\\tests\\": ["backend/tests/", "backend/tests/_support"],
            "api\\tests\\": ["api/tests/", "api/tests/_support"]
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
